model ExecuteTask {
    id           String        @id @default(uuid())
    status       String        @default("pending")
    createAt     DateTime      @default(now())
    count        Int
    rssId        String
    rssGenerator RssGenerator? @relation(fields: [rssId], references: [id])
    isRead       Boolean       @default(false)
    errorReason  String?
    successCount Int           @default(0)
}
