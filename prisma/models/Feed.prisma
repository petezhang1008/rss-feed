model Feed {
    id          String    @id @default(uuid())
    title       String
    link        String
    image       String?
    description String?
    pubDate     DateTime?
    author      String?
    domain      String?
    createdAt   DateTime  @default(now())
    updatedAt   DateTime? @default(now()) @updatedAt
    rssId       String
    rss         Rss       @relation(fields: [rssId], references: [id], onDelete: Cascade)
}
