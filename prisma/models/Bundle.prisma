model Bundle {
    id            String         @id @default(uuid())
    title         String
    description   String?
    userId        String
    pinned        Boolean        @default(false)
    user          User           @relation(fields: [userId], references: [id])
    createdAt     DateTime       @default(now())
    updatedAt     DateTime       @updatedAt
    rssGenerators RssGenerator[]
}
