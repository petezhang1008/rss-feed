model Bundle {
    id          String    @id @default(uuid())
    title       String
    description String?
    userId      String
    pinned      Boolean   @default(false)
    createdAt   DateTime  @default(now())
    updatedAt   DateTime? @default(now()) @updatedAt
    userRss     UserRss[]
    user        User      @relation(fields: [userId], references: [id])
}
